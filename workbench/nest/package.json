    "dev": "nest start --watch",
    "build": "nest build",
    "start": "node dist/main.js"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.17",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@workflow/nest": "workspace:*",
    "@workflow/world-postgres": "workspace:*",
    "ai": "catalog:",
    "express": "^5.1.0",
    "lodash.chunk": "^4.2.0",
    "openai": "^6.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "workflow": "workspace:*"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.0",
    "@types/node": "^22.10.7",
