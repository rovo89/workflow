{
  "name": "@workflow/example-nest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generate:workflows": "node ../scripts/generate-workflows-registry.js ./src/workflows ./src/_workflows.ts --esm",
    "setup:swcrc": "node ../../packages/nest/dist/cli.js init --force",
