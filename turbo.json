{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["WORKFLOW_PUBLIC_MANIFEST"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "$TURBO_ROOT$/packages/tsconfig/base.json",
        "$TURBO_ROOT$/pnpm-workspace.yaml",
        "$TURBO_ROOT$/.node-version"
      ],
      "outputs": [
        "dist/**",
        "build",
        ".svelte-kit",
        ".vercel/output",
        ".output/**"
      ]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
